{
  "name": "adventure-life-api",
  "version": "1.0.0",
  "description": "RESTful API backend for a fictional travel platform built with TypeScript, Express, Objection.js, and PostgreSQL",
  "main": "dist/src/app.js",
  "scripts": {
    "migrate": "knex migrate:latest",
    "migrate:rollback": "knex migrate:down",
    "migrate:make": "knex migrate:make",
    "seed": "knex seed:run",
    "truncate-db": "ts-node scripts/truncate-db.ts",
    "reset-db": "ts-node scripts/reset-db.ts",
    "clean": "rimraf dist",
    "build": "tsc",
    "dev": "nodemon src/app.ts",
    "start": "ts-node src/app.ts",
    "lint": "eslint 'src/**/*.{ts,js}'",
    "format": "prettier --write 'src/**/*.{ts,js,json,md}'",
    "test": "jest --detectOpenHandles"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  },
  "keywords": [
    "typescript",
    "express",
    "postgresql",
    "objection",
    "api",
    "backend"
  ],
  "author": "Tamara Dowis",
  "license": "ISC",
  "devDependencies": {
    "@tsed/common": "^7.13.3",
    "@types/express": "^4.17.15",
    "@types/jest": "^29.5.0",
    "@types/morgan": "^1.9.4",
    "@types/supertest": "^2.0.12",
    "@types/swagger-jsdoc": "^6.0.1",
    "@types/swagger-ui-express": "^4.1.3",
    "@typescript-eslint/eslint-plugin": "^8.30.1",
    "@typescript-eslint/parser": "^8.30.1",
    "eslint": "^9.24.0",
    "eslint-config-prettier": "^10.1.2",
    "eslint-plugin-prettier": "^5.2.6",
    "jest": "^29.5.0",
    "jest-extended": "^3.2.4",
    "nodemon": "^3.1.9",
    "npm-run-all": "^4.1.5",
    "pg": "^8.10.0",
    "pg-hstore": "^2.3.4",
    "prettier": "^3.5.3",
    "rimraf": "^4.1.2",
    "supertest": "^6.3.3",
    "ts-jest": "^29.0.5",
    "ts-node": "^10.9.1",
    "tsc-watch": "^6.0.0",
    "typescript": "^4.9.4"
  },
  "dependencies": {
    "@tsed/objection": "^8.8.0",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "knex": "^3.1.0",
    "morgan": "^1.10.0",
    "objection": "^3.1.5",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^4.6.0",
    "winston": "^3.8.2",
    "yup": "^0.32.11"
  }
}
